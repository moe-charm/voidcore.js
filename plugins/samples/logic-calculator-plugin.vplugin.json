    "ui": {
      "icon": "ðŸ§®"
      "color": "#f39c12"
      "size": "medium"
    }
    "compatibility": {
      "voidcore": "14.0+"
      "browser": "modern"
      "mobile": true
    }
  }
  "config": {
    "precision": 10
    "angleMode": "radians"
    "allowVariables": true
    "customFunctions": []
  }
  "inputs": [
    {
      "name": "expression"
      "type": "string"
      "description": "Mathematical expression to evaluate"
    }
    {
      "name": "variables"
      "type": "object"
      "description": "Variables to substitute in expression"
    }
  ]
  "outputs": [
    {
      "name": "result"
      "type": "number"
