  "inputs": [
    {
      "name": "prompt"
      "type": "string"
      "description": "Text prompt for generation"
    }
    {
      "name": "context"
      "type": "string"
      "description": "Additional context"
    }
    {
      "name": "parameters"
      "type": "object"
      "description": "Generation parameters"
    }
  ]
  "outputs": [
    {
      "name": "generated"
      "type": "string"
      "description": "Generated text"
    }
    {
